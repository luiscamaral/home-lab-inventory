# Rundeck Environment Configuration
# Copy this file to .env and fill in the actual values

# Database Passwords
RUNDECK_DATABASE_PASSWORD=your_secure_database_password_here
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Storage Converter Passwords for encrypted storage
RUNDECK_STORAGE_PASSWORD=your_secure_storage_password_here
RUNDECK_CONFIG_STORAGE_PASSWORD=your_secure_config_password_here

# Optional: Set specific Rundeck version (defaults to latest)
# RUNDECK_VERSION=5.8.0
