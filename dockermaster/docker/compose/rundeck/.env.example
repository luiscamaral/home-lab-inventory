# Rundeck Environment Configuration
# Copy this file to .env and fill in the actual values

# Database Password for PostgreSQL
RUNDECK_DB_PASSWORD=your_secure_database_password_here

# Storage Converter Password for encrypted storage
RUNDECK_STORAGE_PASSWORD=your_secure_storage_password_here

# Config Storage Converter Password
RUNDECK_CONFIG_PASSWORD=your_secure_config_password_here

# Optional: Set specific Rundeck version (defaults to latest)
# RUNDECK_VERSION=5.8.0
