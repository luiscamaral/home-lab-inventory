# Docker Vault Environment Configuration
# Copy this file to .env and fill in the actual values

# Vault Root Token
# This token should be generated during initial Vault setup
# Keep this secure and never commit to version control
VAULT_ROOT_TOKEN=your_vault_root_token_here

# Optional: Additional Vault Configuration
# VAULT_API_ADDR=http://vault.example.com:8200
# VAULT_CLUSTER_ADDR=https://vault.example.com:8201
