{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD009": {
    "br_spaces": 2
  },
  "MD010": true,
  "MD012": {
    "maximum": 2
  },
  "MD013": {
    "code_blocks": false,
    "headers": true,
    "headings": true,
    "line_length": 120,
    "tables": false
  },
  "MD014": true,
  "MD018": true,
  "MD019": true,
  "MD020": true,
  "MD021": true,
  "MD022": true,
  "MD023": true,
  "MD024": {
    "allow_different_nesting": true,
    "siblings_only": true
  },
  "MD025": {
    "front_matter_title": "",
    "level": 1
  },
  "MD026": {
    "punctuation": ".,;:!\u3002\uff0c\uff1b\uff1a\uff01"
  },
  "MD027": true,
  "MD028": true,
  "MD029": {
    "style": "ordered"
  },
  "MD030": {
    "ol_multi": 1,
    "ol_single": 1,
    "ul_multi": 1,
    "ul_single": 1
  },
  "MD031": true,
  "MD032": true,
  "MD033": {
    "allowed_elements": [
      "details",
      "summary",
      "br",
      "sub",
      "sup",
      "img",
      "kbd",
      "mark"
    ]
  },
  "MD034": true,
  "MD035": {
    "style": "---"
  },
  "MD036": {
    "punctuation": ".,;:!?\u3002\uff0c\uff1b\uff1a\uff01\uff1f"
  },
  "MD037": true,
  "MD038": true,
  "MD039": true,
  "MD040": true,
  "MD041": {
    "front_matter_title": "",
    "level": 1
  },
  "MD042": true,
  "MD043": false,
  "MD044": {
    "code_blocks": false,
    "names": [
      "Docker",
      "Docker Compose",
      "GitHub",
      "GitHub Actions",
      "CI/CD",
      "API",
      "URL",
      "HTTP",
      "HTTPS",
      "JSON",
      "YAML",
      "XML",
      "HTML",
      "CSS",
      "JavaScript",
      "TypeScript",
      "Node.js",
      "npm",
      "Kubernetes",
      "Ansible",
      "Terraform",
      "AWS",
      "Azure",
      "GCP",
      "Linux",
      "Ubuntu",
      "CentOS",
      "RHEL",
      "SSH",
      "SSL/TLS",
      "OAuth",
      "JWT",
      "REST",
      "GraphQL",
      "PostgreSQL",
      "MySQL",
      "MongoDB",
      "Redis",
      "Nginx",
      "Apache",
      "Prometheus",
      "Grafana",
      "ELK Stack",
      "Elasticsearch",
      "Logstash",
      "Kibana"
    ]
  },
  "MD045": true,
  "MD046": {
    "style": "fenced"
  },
  "MD047": true,
  "MD048": {
    "style": "backtick"
  },
  "MD049": {
    "style": "underscore"
  },
  "MD050": {
    "style": "asterisk"
  },
  "MD051": true,
  "MD052": true,
  "MD053": true,
  "default": true
}
